IDD_PORTDIALOG DIALOG 40, 22, 310, 145
STYLE DS_MODALFRAME | WS_MINIMIZEBOX | WS_MAXIMIZEBOX | WS_POPUP | 
    WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION "ﾎﾟｰﾄ中 -"
FONT 9, "MS Shell Dlg"
BEGIN
    LTEXT	    "検索中...", IDC_SEARCHFOUND, 10, 10, 88, 9
    LTEXT	    "ﾌｧｲﾙ中の問題点:", -1, 222, 10, 62, 9
    LTEXT	    "0", IDC_ISSUECNT, 289, 10, 30, 9
    LTEXT	    "行番号:", -1, 10, 22, 24, 9
    LTEXT	    "0", IDC_LINENO, 42, 22, 20, 9
    EDITTEXT	    IDC_LINE, 10, 32, 290, 12, NOT WS_BORDER | ES_NOHIDESEL | ES_READONLY | ES_MULTILINE
    LTEXT	    "問題点:", -1, 10, 46, 27, 9
    EDITTEXT	    IDC_ISSUE, 10, 55, 290, 12, ES_AUTOHSCROLL | WS_GROUP | ES_READONLY
    LTEXT	    "解決法:", IDC_SUGGESTLABEL, 10, 79, 60, 9
    EDITTEXT	    IDC_SUGGESTION, 10, 88, 290, 30, ES_MULTILINE | WS_VSCROLL | WS_GROUP | ES_READONLY
    PUSHBUTTON	    "停止", IDCANCEL, 10, 126, 40, 14, WS_GROUP
    DEFPUSHBUTTON   "続行", IDC_CONTINUE, 60, 126, 40, 14, WS_GROUP
    PUSHBUTTON	    "再実行", IDC_RESTART, 110, 126, 40, 14, WS_GROUP
    PUSHBUTTON	    "ｵﾌﾟｼｮﾝ...", IDC_OPTIONS, 160, 126, 40, 14, WS_GROUP
    PUSHBUTTON	    "ﾍﾙﾌﾟ", IDC_HELPM, 210, 126, 40, 14, WS_GROUP
    PUSHBUTTON	    "完了", IDC_DONE, 260, 126, 40, 14, WS_GROUP

END


