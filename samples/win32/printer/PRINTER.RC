    MENUITEM    "&PolyPolygon()"           IDM_POLYPOLYGON
    MENUITEM    "Re&ctangle()"             IDM_RECTANGLE
    MENUITEM    "Ro&undRect()"             IDM_ROUNDRECT
    MENUITEM    "&StrectBlt()"             IDM_STRETCHBLT
