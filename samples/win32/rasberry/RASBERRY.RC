APPNAME ICON ICONFILE

; Bring in the dialogs:
RCINCLUDE ABOUT.DLG
#include  "statdlg.h"
RCINCLUDE STATDLG.DLG                                                                     
#include  "phbkdlg.h"
RCINCLUDE PHBKDLG.DLG
#include  "dialdlg.h"
